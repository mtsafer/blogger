<h1>All Articles</h1>

<ul id="articles">
	<% @articles.each do |a| %>
		<li><%= link_to a.title, a, class: "article_title" %></li>
	<% end %>
</ul>
<%= link_to "Create a new article", new_article_path, class: "new_article btn btn-primary" %>
<%= link_to "See all tags", tags_path, class: "btn btn-primary" %>